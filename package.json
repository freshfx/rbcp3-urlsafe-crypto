{
  "name": "rbcp3-urlsafe-crypto",
  "version": "1.0.0",
  "description": "urlsafe aes-256-cbc encrypt/decrypt a string/object",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "bash scripts/build",
    "clean": "rm -rf dist",
    "docs:jsdoc": "jsdoc src/index.js -d dist/docs",
    "docs": "jsdoc2md src/index.js > README.md",
    "docs:watch": "onchange 'src/**/*.js' 'conf.json' -v -- npm run docs",
    "lint": "eslint -c .eslintrc.yml src/ test/",
    "lint:watch": "onchange 'src/**/*.js' 'test/**/*.js' -v -- npm run lint",
    "prepare": "bash scripts/prepare",
    "scripts": "babel src/ -d dist/ --copy-files",
    "scripts:watch": "babel src/ --watch -d dist/ -s true --copy-files",
    "scripts:watch:delete": "onchange \"src/**/*.*\" -- scripts/remove-on-unlink '{{event}} {{changed}}'",
    "start": "bash scripts/start",
    "test": "NODE_ENV=test mocha",
    "test:watch": "onchange 'dist/**/*.js' 'test/**/*.js' -- npm run test",
    "watch": "bash scripts/watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freshfx/rbcp3-urlsafe-crypto.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "keywords": [
    "crypto"
  ],
  "author": "Michael Poschacher <poschi@freshfx.at>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/freshfx/rbcp3-urlsafe-crypto/issues"
  },
  "homepage": "https://github.com/freshfx/rbcp3-urlsafe-crypto#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-jsdoc-properties": "^0.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "deep-freeze": "0.0.1",
    "eslint": "^4.6.1",
    "jsdoc": "^3.5.4",
    "mocha": "^3.5.3",
    "npm-run-all": "^4.1.1",
    "onchange": "^3.2.1",
    "should": "^13.0.1",
    "should-sinon": "0.0.6",
    "sinon": "^3.2.1",
    "jsdoc-babel": "^0.3.0"
  },
  "dependencies": {}
}
